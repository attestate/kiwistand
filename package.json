{
  "name": "kiwistand",
  "version": "0.12.0",
  "description": "kiwistand is the p2p node client for kiwinews.xyz, a web3 writer friendly Hacker News that nobody controls but everybody co-owns",
  "main": "./src/index.mjs",
  "scripts": {
    "postinstall": "npm rebuild better-sqlite3",
    "clean:assets": "rm src/public/assets/*",
    "build": "npm run clean:assets && cd src/web && NODE_ENV=production npm run build && cp ../public/.vite/manifest.json ../public/manifest.json",
    "test": "DEBUG=\"-@attestate/delegator2*\" ava --timeout=30s --serial --verbose",
    "test:compression": "NODE_TLS_REJECT_UNAUTHORIZED=0 ava test/compression_test.mjs --timeout=30s --verbose",
    "connect": "ssh root@91.107.210.214",
    "deploy": "npm run build && npm run rsync",
    "rsync": "rsync -avz . root@91.107.210.214:./kiwistand --exclude=.claude --exclude=src/iOS --exclude=certificates --exclude=anonlocal --exclude=cache --exclude=docs --exclude=anon --exclude \"node_modules\" --exclude=\".git\" --exclude=bootstrap --exclude=data",
    "dev": "cross-env DEBUG=\"*@attestate/*,-@attestate/delegator2*\" concurrently \"npm run watch:web\" \"npm run start\"",
    "dev:ssl": "cross-env DEBUG=\"*@attestate/*,-@attestate/delegator2*,-@attestate/crawler*,-@attestate/extraction-worker*\" concurrently \"npm run watch:web:ssl\" \"npm run start\"",
    "start": "concurrently \"npm run start:load-balancer\" \"npm run start:primary\"",
    "start:load-balancer": "node -r dotenv/config ./src/load-balancer.mjs",
    "start:primary": "node -r dotenv/config ./src/launch.mjs",
    "watch": "nodemon --watch src/views --watch src/http.mjs --exec 'npm run dev:anon'",
    "watch:ssl": "nodemon --watch src/views --exec 'npm run dev:anon:ssl'",
    "watch:anon:local": "nodemon --watch src/views --watch src/http.mjs --exec 'npm run dev:anon:local'",
    "reconcile": "cross-env NODE_ENV=reconcile API_PORT=8443 DATA_DIR=anon BIND_ADDRESS_V4=\"0.0.0.0\" PORT=0 IS_BOOTSTRAP_NODE=false USE_EPHEMERAL_ID=true node -r dotenv/config ./src/launch.mjs",
    "watch:web": "cd src/web && npm run dev",
    "watch:web:ssl": "cd src/web && npm run dev:ssl",
    "dev:bootstrap": "cross-env API_PORT=8443 HTTP_PORT=80 DATA_DIR=bootstrap BIND_ADDRESS_V4=\"127.0.0.1\" IS_BOOTSTRAP_NODE=true USE_EPHEMERAL_ID=false npm run dev",
    "dev:anon:local": "cross-env API_PORT=8443 HTTP_PORT=4001 DATA_DIR=anonlocal BIND_ADDRESS_V4=\"127.0.0.1\" PORT=0 IS_BOOTSTRAP_NODE=false USE_EPHEMERAL_ID=true npm run dev",
    "dev:anon": "cross-env NODE_ENV=dev WORKER_COUNT=2 API_PORT=8443 HTTP_PORT=4000 DATA_DIR=anon BIND_ADDRESS_V4=\"0.0.0.0\" PORT=0 IS_BOOTSTRAP_NODE=false USE_EPHEMERAL_ID=true npm run dev",
    "dev:anon:ssl": "cross-env NODE_ENV=dev API_PORT=8443 HTTP_PORT=4000 DATA_DIR=anon CUSTOM_PROTOCOL=https:// BIND_ADDRESS_V4=\"0.0.0.0\" PORT=0 IS_BOOTSTRAP_NODE=false USE_EPHEMERAL_ID=true npm run dev:ssl",
    "dev:staging": "nodemon --watch src/views --exec 'cross-env NODE_ENV=dev API_PORT=8443 HTTP_PORT=443 DATA_DIR=anon BIND_ADDRESS_V4=\"0.0.0.0\" PORT=0 IS_BOOTSTRAP_NODE=false USE_EPHEMERAL_ID=true npm run dev'",
    "migration:1": "node -r dotenv/config scripts/migration1.mjs",
    "sync": "cross-env DEBUG=\"@attestate/*\" node -r dotenv/config ./src/gen_db.mjs",
    "sync:cache": "rsync -avz root@91.107.210.214:./kiwistand/cache/database.db* root@91.107.210.214:./kiwistand/cache/interactions.db* ./cache/ && cp ./cache/database.db ./cache/database.db.prod",
    "test:bot-submission": "node -r dotenv/config ./scripts/test_submission.mjs",
    "create:farcaster-signer": "node ./scripts/create-farcaster-signer.mjs",
    "auth:twitter": "node ./scripts/twitter-auth.mjs",
    "purge:url": "node ./scripts/purge-cloudflare-url.mjs",
    "tunnel": "ssh -R 8443:localhost:8443 -R 5173:localhost:5173 -R 443:localhost:443 -o ServerAliveInterval=60 -N root@65.108.82.10",
    "watch:staging": "cross-env CUSTOM_HOST_NAME=\"staging.kiwistand.com:5173\" CUSTOM_PROTOCOL=\"https://\" npm run dev:staging"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/attestate/kiwistand.git"
  },
  "keywords": [
    "peer-to-peer",
    "eip-712",
    "data-storage",
    "ethereum"
  ],
  "engines": {
    "node": "^20.0.0"
  },
  "author": "The Kiwi News Developers",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/attestate/kiwistand/issues"
  },
  "homepage": "https://github.com/attestate/kiwistand#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "0.33.1",
    "@attestate/crawler": "0.7.3",
    "@attestate/crawler-call-block-logs": "0.6.2",
    "@attestate/delegator2": "0.7.0",
    "@chainsafe/libp2p-gossipsub": "5.2.1",
    "@chainsafe/libp2p-noise": "10.2.0",
    "@ethereumjs/rlp": "4.0.1",
    "@ethereumjs/trie": "5.0.4",
    "@ethersproject/wallet": "5.7.0",
    "@farcaster/hub-nodejs": "0.13.4",
    "@farcaster/quick-auth": "0.0.7",
    "@libp2p/bootstrap": "5.0.0",
    "@libp2p/mplex": "7.1.0",
    "@libp2p/peer-id": "1.1.16",
    "@libp2p/pubsub-peer-discovery": "8.0.0",
    "@libp2p/tcp": "6.0.4",
    "@neynar/nodejs-sdk": "2.41.0",
    "@tensorflow/tfjs-node": "4.22.0",
    "ajv": "8.12.0",
    "ajv-formats": "2.1.1",
    "arweave": "1.14.4",
    "better-sqlite3": "9.4.0",
    "blocked-at": "1.2.0",
    "canonicalize": "2.0.0",
    "cbor-x": "1.5.1",
    "cloudflare": "4.2.0",
    "compression": "1.8.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "date-fns": "2.29.3",
    "debug": "4.3.4",
    "dotenv": "16.0.3",
    "emoji-regex": "10.3.0",
    "eth-fun": "0.10.1",
    "ethereum-cryptography": "1.1.2",
    "ethers": "5.7.2",
    "express": "4.18.2",
    "express-async-errors": "3.1.1",
    "fastq": "1.17.1",
    "htm": "3.1.1",
    "html-entities": "2.6.0",
    "http-proxy-middleware": "3.0.3",
    "isbot": "3.7.0",
    "isomorphic-dompurify": "2.12.0",
    "it-all": "2.0.0",
    "it-length-prefixed": "8.0.4",
    "it-map": "2.0.0",
    "libp2p": "0.40.0",
    "linkify-string": "4.1.3",
    "linkifyjs": "4.2.0",
    "lmdb": "2.7.9",
    "make-asynchronous": "1.0.1",
    "morgan": "1.10.0",
    "node-cache": "5.1.2",
    "node-fetch-cache": "3.1.2",
    "node-html-parser": "6.1.13",
    "node-telegram-bot-api": "0.66.0",
    "nodemon": "2.0.21",
    "normalize-url": "8.0.0",
    "oauth-1.0a": "2.2.6",
    "open-graph-scraper-lite": "2.0.0",
    "piscina": "4.7.0",
    "pm2": "5.3.0",
    "porto": "0.2.35",
    "postmark": "4.0.5",
    "preact": "10.18.1",
    "request-filtering-agent": "2.0.1",
    "satori": "0.10.9",
    "sharp": "0.32.6",
    "slugify": "1.6.6",
    "svg-line-chart": "0.4.1",
    "vhtml": "github:attestate/vhtml-memory-safe#master",
    "web-push": "3.6.7"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2",
    "ava": "^5.3.1",
    "concurrently": "8.0.1",
    "csv-parser": "3.2.0",
    "csv-writer": "1.6.0",
    "puppeteer": "24.16.1",
    "vite": "4.3.1"
  },
  "ava": {
    "files": [
      "**/*_test.mjs"
    ],
    "environmentArguments": {
      "DEBUG": "attestate*",
      "HTTP_MESSAGES_MAX_PAGE_SIZE": "500"
    },
    "nodeArguments": [
      "--no-warnings"
    ],
    "require": [
      "dotenv/config"
    ]
  }
}
