{
  "name": "rugpulld",
  "version": "0.0.1",
  "description": "rugpulld is a network daemon for the rugpull network",
  "main": "./src/index.mjs",
  "scripts": {
    "test": "ava --verbose",
    "dev": "node -r dotenv/config --no-warnings ./src/launch.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rugpullindex/rugpulld.git"
  },
  "keywords": [
    "rugpull",
    "data",
    "tokens",
    "layer2",
    "blockchain",
    "ethereum",
    "p2p"
  ],
  "engines": {
    "node": ">=16"
  },
  "author": "Tim Daubensch√ºtz",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/rugpullindex/rugpulld/issues"
  },
  "homepage": "https://github.com/rugpullindex/rugpulld#readme",
  "dependencies": {
    "@chainsafe/libp2p-gossipsub": "5.2.1",
    "@chainsafe/libp2p-noise": "10.2.0",
    "@libp2p/bootstrap": "5.0.0",
    "@libp2p/mplex": "7.1.0",
    "@libp2p/peer-id": "1.1.16",
    "@libp2p/tcp": "6.0.4",
    "dotenv": "16.0.3",
    "esmock": "1.7.2",
    "libp2p": "0.40.0",
    "pino": "7.6.0"
  },
  "devDependencies": {
    "ava": "3.15.0",
    "pino-pretty": "7.3.0"
  },
  "ava": {
    "nodeArguments": ["--loader=esmock", "--no-warnings"],
    "require": ["dotenv/config"]
  }
}
